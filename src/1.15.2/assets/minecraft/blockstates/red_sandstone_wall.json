{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/red_sandstone_wall_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "block/red_sandstone_wall_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/walls/red_sandstone_wall_side_ew", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/red_sandstone_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/walls/red_sandstone_wall_side_ew", "y": 180, "uvlock": true }
        }
    ]
}
