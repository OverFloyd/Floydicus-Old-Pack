{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/diorite_wall_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "block/diorite_wall_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/walls/diorite_wall_side_ew", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/diorite_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/walls/diorite_wall_side_ew", "y": 180, "uvlock": true }
        }
    ]
}
